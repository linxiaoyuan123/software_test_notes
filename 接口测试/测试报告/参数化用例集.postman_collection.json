					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"断言归属地\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data.sp).to.eql(data.file_sp);\r",
							"});"
